    <div class="brochure-wrapper">
	<div class="searchbar">
		<div class="main-search-bar__wrapper main-search-bar--vertical-wrapper main-search-bar--booking-block" data-village-name="whinfell" data-village-code="WF" data-tag-list="[{&#34;name&#34;:&#34;2-bedrooms&#34;,&#34;description&#34;:&#34;Number of bedrooms&#34;},{&#34;name&#34;:&#34;woodland&#34;,&#34;description&#34;:&#34;Accommodation type&#34;}]">
			<div class="main-search-bar--vertical" v-if="!showErrorMessage">
				<div class="main-search-bar__headline">
					<h2 class="main-search-bar__heading">Book your break 
                        <span class="main-search-bar__heading-price">{{price}}</span>
                    </h2>
				</div>
				<form class="main-search-bar__form" role="search">
					<div class="main-search-bar__item main-search-bar__item--village selected disabled">
						<label for="Village" class="hidden">Locations</label>
						<input type="text" class="hidden" name="Village" id="Village" value="Choose village(s)" aria-disabled="true" disabled/>
						<button class="btn sb-input-button btn--search-green disabled" aria-disabled="true" disabled>
							<div class="sb-input-icon icon-sb-location"></div>
							<span>{{village}}</span>
							<div class="sb-input-icon icon-dropdown-arrow"></div>
						</button>
					</div>
					<div class="main-search-bar__item main-search-bar__item--dates selected disabled">
						<label for="Dates" class="hidden">Choose Dates</label>
						<input type="text" class="hidden" name="Dates" id="Dates" value="Choose dates" aria-disabled="true" disabled/>
						<button class="btn sb-input-button btn--search-green disabled" aria-disabled="true" disabled>
							<div class="sb-input-icon icon-sb-calendar"></div>
							<div class="sb-input-date__item">
								<span class="sb-input-date__item-day"> {{arrivalDay}} </span>
								<span class="sb-select-date__item-month"> {{arrivalDate}}</span>
							</div>
							<div class="sb-input-icon icon-sb-right-arrow sb-input-right-arrow-added-day"></div>
							<div class="sb-input-date__item">
								<span class="sb-input-date__item-day"> {{departureDay}} </span>
								<span class="sb-input-date__item-month"> {{departureDate}} </span>
							</div>
							<div class="sb-input-icon icon-dropdown-arrow"></div>
						</button>
					</div>
					<div class="main-search-bar__item main-search-bar__item--lodges selected disabled">
						<label for="Lodges" class="hidden">Lodges</label>
						<input type="text" class="hidden" name="Lodges" id="Lodges" value="Lodges" aria-disabled="true" disabled/>
						<button class="btn sb-input-button btn--search-green disabled" aria-disabled="true" disabled>
							<div class="sb-input-icon icon-sb-lodge"></div>
							<span>{{accommodationName}}</span>
							<div class="sb-input-icon icon-dropdown-arrow"></div>
						</button>
					</div>
					<div class="main-search-bar__item main-search-bar__item--guests selected disabled" v-if="(behaviour == 'book')" v-bind:class="{ 'disabled' : guestData }">
						<label for="Guests" class="hidden">Add Guests &amp; Dogs</label>
						<input type="text" class="hidden" name="Guests" id="Guests" value="Add Guests &amp; Dogs"/>
						<button class="btn sb-input-button btn--search-green" v-bind:class="{ 'disabled' : guestData }" :disabled="{ true : guestData }">
							<div class="sb-input-icon icon-sb-guest"></div>
							<span>{{guestData}}</span>
							<div class="sb-input-icon icon-dropdown-arrow"></div>
						</button>
					</div>

					<div class="main-search-bar__item main-search-bar__item--guests selected" v-if="(behaviour == 'search')" v-bind:class="{ 'selected' : guestData }">
						<label for="Guests" class="hidden">"Add guests &amp; dogs"
						</label>
						<input type="text" class="hidden" name="Guests" id="Guests" value="Add Guests &amp; Dogs"/>
						<button aria-haspopup="true" aria-labelledby="sb-input-guest-label" class="btn sb-input-button btn--search-green  main-search-bar__item--guests-btn" @click.stop.prevent="onGuestClick()">
							<div class="sb-input-icon icon-sb-guest"></div>
							<span id="sb-input-guest-label" class="sb-input__guests-summary" v-if="guestData">{{guestData}}</span>
                            <span id="sb-input-guest-label" class="sb-input__guests-summary" v-else>Add Guests &amp; Dogs</span>
							<div class="sb-input-icon icon-dropdown-arrow"></div>
						</button>
						<div class="main-search-bar__item--guests-cmp main-search-bar__item--dropdown">
							
<guests-dropdown-booking-block class="guests-dropdown-wrapper" data-max-guests="4" data-max-infants="2" data-max-dogs="2" data-max-bedrooms="2" :village-code="villageCode" :adults-count="2" dog-summary-text="1 dog ${currency}${oneDogPrice}, 2 dogs ${currency}${twoDogPrice}" @update-booking-guest-list="setGuestSummary($event)" inline-template>
   <div class="sb-add-guest">
       <div class="sb-add-guest__header">
          <button class="icon-chevron-left" @click.stop.prevent="closeDropdown()"></button>
          <h3 class="sb-add-guest__header-title">Add Guests &amp; Dogs</h3>
          <button class="material-icons close-btn" @click.stop.prevent="closeDropdown()">close</button>
       </div>
       <p class="sb-add-guest__subtitle">Please enter age at time of arrival</p>

       <div class="sb-add-guest__accordion-item close" v-for="lodge in lodges">
          <div v-else class="sb-add-guest__accordion-header" @click.stop.prevent="toggleGuestAccordion($event)">
            <h6 class="sb-add-guest__lodge-title"><i class="icon-sb-lodge"></i>Lodge 1</h6>
            <span class="sb-add-guest__down-icon icon-chevron-down"></span>
          </div>
          <div class="sb-add-guest__accordion-content">
             <div class="sb-add-guest__guests">
                <div class="sb-form-increment">
                   <div class="sb-form-increment__container">
                      <label for="" class="sb-form-increment__title">Adults<span class="sb-form-increment__desc">18+ years</span></label>
                      <div class="sb-form-increment__button-container">
                         <button class="sb-form-increment__button js-form__decrement icon-minus" aria-label="decrease number of adults" :disabled="lodge.adults < 2" :class="lodge.adults < 2 ? 'inactive': ''" data-field="adults" @click.stop.prevent="removeGuest(); updateGuestList(); minBedrooms();">
                         </button>
                         <input type="number" name="search_increment_value" id="adults" :value="lodge.adults" class="sb-form-increment__value js-form__value" min="1" max="4" readonly/>
                         <button class="sb-form-increment__button js-form__increment icon-plus" aria-label="increase number of adults" :disabled="lodge.totalGuests >= maxGuests" :class="lodge.totalGuests >= maxGuests ? 'inactive': ''" data-field="adults" @click.stop.prevent="addGuest(); updateGuestList(); minBedrooms();">
                         </button>
                      </div>
                   </div>
                </div>
                <div class="sb-form-increment">
                   <div class="sb-form-increment__container">
                      <label for="" class="sb-form-increment__title">Children<span class="sb-form-increment__desc">6 - 17 years</span></label>
                      <div class="sb-form-increment__button-container">
