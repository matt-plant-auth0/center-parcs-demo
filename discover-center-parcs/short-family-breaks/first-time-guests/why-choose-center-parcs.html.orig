                   <div class="AB-cards">
                      <div class="AB-card" v-for="(each, index) in booking.multiactivities">
                         <div class="AB-card__image" v-for="eachactivity in each.multiBuyActivities" v-if="(each.multiBuyActivityCode.indexOf(eachactivity.mbActivity) === -1)">
                          <div class="basket__img basket__img--cover" :style="'background-image:url(' + escape(eachactivity.icon || eachactivity.thumbnailImage) + ')'">
                            <span class="sr-only">Image for {{eachactivity.multiBuyDesc}}</span>
                          </div>
                         </div>
                         <div class="AB-card__content" v-for="eachactivity in each.multiBuyActivities" v-if="(each.multiBuyActivityCode.indexOf(eachactivity.mbActivity) === -1)">
